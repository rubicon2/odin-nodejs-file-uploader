<% if (locals.folders) { %>
  <ul class="list-no-style directory-list">
    <% for (const folder of folders) { %>
      <%- include('directory-list-folder.ejs', { folder }) %>
    <% }; %>
    <% if (locals.files) { %>
      <% for (const file of files) { %>
        <li>
          <a href="/file/<%= file.id %>">
            <%= file.name %>
          </a>
          <%- include('../file/delete-file-form.ejs', { file }) %>
        </li>
      <% }; %>
    <% }; %>
  </ul>
<% }; %>